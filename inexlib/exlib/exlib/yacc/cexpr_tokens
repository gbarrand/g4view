#ifndef exlib_yacc_cexpr_tokens
#define exlib_yacc_cexpr_tokens


namespace inlib {class valop;}

#define STRG 257
#define T_NAME 258
#define NUMBER_INTEGER 259
#define NUMBER_REAL 260
#define T_PI 261
#define T_TRUE 262
#define T_FALSE 263
#define T_AND 264
#define T_OR 265
#define UMINUS 266
typedef union {
  unsigned int m_index;
  unsigned int m_integer;
  double m_number;
  inlib::valop* m_expression;
} YYSTYPE;
extern YYSTYPE cexpr_eval_lval;

#endif
