if [ "${use_csz}" = "yes" ] ; then
  if [ "${ourex_home}" = "" ] ; then
    echo 'bush/use/csz : variable ourex_home not defined.';exit
  fi
  csz_home=${ourex_home}/csz
  ourex_pack_name=csz
  . ${bush_home}/use/ourex_use
fi

